{
    "version": 2,
    "builds": [
      {
        "src": "api/conversations.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/conversations",
        "dest": "/api/conversations.js",
        "methods": ["GET"],
        "headers": {
          "cache-control": "s-maxage=1, stale-while-revalidate"
        
      }
    }
    ]
  }
  